    "@react-router/serve": "^7.11.0",
    "isbot": "^5.1.32",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-router": "^7.11.0"
  },
  "devDependencies": {
    "@biomejs/cli": "npm:@biomejs/biome@2.3.10",
    "@react-router/dev": "^7.11.0",
    "@react-router/testing": "file:vendor/react-router-testing",
    "@tailwindcss/vite": "^4.1.18",
