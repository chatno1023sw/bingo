{
  "name": "bingo",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:local": "BASE_PATH=./ react-router build && node ./scripts/copy-local.mjs",
    "build:all": "react-router build && BASE_PATH=./ react-router build && node ./scripts/copy-local.mjs",
    "dev": "react-router dev",
    "start": "node ./scripts/serve-client.mjs",
    "typecheck": "react-router typegen && tsc",
    "lint": "biome lint --write --error-on-warnings --files-ignore-unknown=true app scripts docs public *.json *.ts",
    "format": "biome format --write --files-ignore-unknown=true app scripts docs public *.json *.ts",
    "pre": "npm run lint && npm run typecheck && npm run format",
    "test": "vitest run --passWithNoTests",
    "similarity": "node ./scripts/run-similarity.mjs"
  },
  "dependencies": {
    "@biomejs/biome": "^2.3.11",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/styled": "^11.14.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@react-router/node": "^7.12.0",
    "@react-router/serve": "^7.12.0",
    "class-variance-authority": "^0.7.1",
    "howler": "^2.2.4",
    "isbot": "^5.1.32",
    "lucide-react": "^0.562.0",
    "papaparse": "^5.5.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.70.0",
    "react-router": "^7.12.0",
    "shadcn": "^3.6.3"
  },
  "devDependencies": {
    "@biomejs/cli": "npm:@biomejs/biome@2.3.11",
    "@react-router/dev": "^7.12.0",
    "@react-router/testing": "file:vendor/react-router-testing",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^4.0.16"
  }
}
